    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
    },
    "autoload-dev": {
        "psr-4": {
            "Inspector\\Slim\\Tests\\": "tests/"
        }
    },
    "scripts": {
    }
